{
  "extends": "next/core-web-vitals",
  "plugins": ["simple-import-sort"],
  "rules": {
    "simple-import-sort/imports": "warn",
    "simple-import-sort/exports": "warn",
    "no-unused-vars": "error",
    "no-implicit-coercion": "error",
    "no-console": [
      "error",
      {
        "allow": ["warn", "error", "info"]
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
